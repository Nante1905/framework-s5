[
  {
    "id": 1,
    "name": "Ionic React",
    "extension": "tsx",
    "skeleton": "ionic-react-skeleton",
    "variables": {
      "endLine": ";",
      "exportKey": "export",
      "imports": {
        "member": {
          "importKey": "import",
          "importStart": "{",
          "importEnd": "}",
          "importFrom": "from"
        },
        "global": {
          "importKey": "import",
          "importStart": "",
          "importEnd": "",
          "importFrom": ""
        },
        "single": {
          "importKey": "import",
          "importStart": "",
          "importEnd": "",
          "importFrom": "from"
        }
      }
    },
    "folders": {
      "liste": "components/[entityMin]/[entityMin]-liste.component.tsx",
      "form": "components/[entityMin]/[entityMin]-form.component.tsx",
      "type": "shared/types/[entityMaj]",
      "formComponentName": "[entityMaj]Form",
      "formComponent": "[entityMin]-form.component"
    },
    "files": {
      "env": "src/shared/env/env.ts"
    },
    "optionalImports": {
      "text": [
        {
          "type": "member",
          "elements": ["TextField"],
          "source": "@mui/material"
        }
      ],
      "number": [
        {
          "type": "member",
          "elements": ["TextField"],
          "source": "@mui/material"
        }
      ],
      "select": [
        {
          "type": "member",
          "elements": ["Select", "InputLabel", "MenuItem"],
          "source": "@mui/material"
        }
      ],
      "date": [
        {
          "type": "member",
          "elements": ["DatePicker", "LocalizationProvider"],
          "source": "@mui/x-date-pickers"
        },
        {
          "type": "member",
          "elements": ["Controller"],
          "source": "react-hook-form"
        },
        {
          "type": "member",
          "elements": ["AdapterDayjs"],
          "source": "@mui/x-date-pickers/AdapterDayjs"
        }
      ]
    },
    "pages": {
      "pageInfo": {
        "name": "PageInfo",
        "path": "[projectName]/[projectFrontName]/src/shared/data/PageInfo",
        "template": "pageinfo-template.templ",
        "imports": [
          {
            "type": "member",
            "elements": ["archiveSharp"],
            "source": "ionicons/icons"
          },
          {
            "type": "member",
            "elements": ["PageInfo"],
            "source": "../types/PageInfo"
          }
        ]
      },
      "landingPage": {
        "name": "LandingPage",
        "path": "[projectName]/[projectFrontName]/src/components/landing-page/LandingPage"
      },
      "menu": {
        "name": "Menu",
        "path": "[projectName]/[projectFrontName]/src/components",
        "imports": [
          {
            "type": "member",
            "elements": [
              "IonContent",
              "IonIcon",
              "IonItem",
              "IonLabel",
              "IonList",
              "IonListHeader",
              "IonMenu",
              "IonMenuToggle"
            ],
            "source": "@ionic/react"
          },
          {
            "type": "member",
            "elements": ["archiveSharp"],
            "source": "ionicons/icons"
          },
          {
            "type": "member",
            "elements": ["useLocation"],
            "source": "react-router-dom"
          },
          {
            "type": "global",
            "source": "./Menu.css"
          }
        ]
      },
      "type": {
        "name": "[entityMaj]",
        "path": "[projectName]/[projectFrontName]/src/shared/types/[entityMaj].ts",
        "imports": []
      },
      "form": {
        "name": "[entity]-form.component",
        "path": "[projectName]/[projectFrontName]/src/components/[entityMin]/[entityMin]-form.component.tsx",
        "imports": [
          {
            "type": "member",
            "elements": ["Button", "CircularProgress", "FormControl"],
            "source": "@mui/material"
          },
          {
            "type": "single",
            "elements": ["axios"],
            "source": "axios"
          },
          {
            "type": "member",
            "elements": ["useEffect", "useState"],
            "source": "react"
          },
          {
            "type": "single",
            "elements": ["ErrorSnackBar"],
            "source": "../../shared/components/snackbar/ErrorSnackBar"
          },
          {
            "type": "single",
            "elements": ["SuccessSnackBar"],
            "source": "../../shared/components/snackbar/SuccessSnackBar"
          },
          {
            "type": "member",
            "elements": ["URL_API"],
            "source": "../../shared/env/env"
          },
          {
            "type": "member",
            "elements": ["useForm"],
            "source": "react-hook-form"
          }
        ]
      },
      "list": {
        "name": "[entity]-liste.component",
        "path": "[projectName]/[projectFrontName]/src/components/[entityMin]/[entityMin]-liste.component.tsx",
        "imports": [
          {
            "type": "single",
            "elements": ["DeleteForeverIcon"],
            "source": "@mui/icons-material/DeleteForever"
          },
          {
            "type": "single",
            "elements": ["EditIcon"],
            "source": "@mui/icons-material/Edit"
          },
          {
            "type": "member",
            "elements": [
              "Button",
              "CircularProgress",
              "Dialog",
              "DialogContent",
              "Table",
              "TableBody",
              "TableCell",
              "TableHead",
              "TableRow",
              "Tooltip"
            ],
            "source": "@mui/material"
          },
          {
            "type": "single",
            "elements": ["axios"],
            "source": "axios"
          },
          {
            "type": "single",
            "elements": ["_"],
            "source": "lodash"
          },
          {
            "type": "member",
            "elements": ["useEffect", "useState"],
            "source": "react"
          },
          {
            "type": "single",
            "elements": ["ErrorSnackBar"],
            "source": "../../shared/components/snackbar/ErrorSnackBar"
          },
          {
            "type": "single",
            "elements": ["SuccessSnackBar"],
            "source": "../../shared/components/snackbar/SuccessSnackBar"
          },
          {
            "type": "single",
            "elements": ["DeleteModal"],
            "source": "../../shared/components/deleteModal/delete-modal.component"
          },
          {
            "type": "member",
            "elements": ["URL_API"],
            "source": "../../shared/env/env"
          },
          {
            "type": "global",
            "source": "../../theme/form.scss"
          },
          {
            "type": "global",
            "source": "../../theme/list.scss"
          },
          {
            "type": "single",
            "elements": ["CustomPagination"],
            "source": "../../shared/components/pagination/CustomPagination"
          }
        ]
      }
    }
  }
]
